{
  "name": "@h3ravel/console",
  "version": "7.0.0",
  "description": "CLI utilities for scaffolding, running migrations, tasks and for H3ravel.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://h3ravel.toneflix.net",
  "repository": {
    "type": "git",
